<?xml version="1.0" encoding="utf-8"?>
<mx:Module xmlns:fx="http://ns.adobe.com/mxml/2009" 
		   xmlns:s="library://ns.adobe.com/flex/spark" 
		   xmlns:mx="library://ns.adobe.com/flex/mx" layout="absolute" width="100%" height="100%" creationComplete="init"
		   xmlns:receipt="oa.receipt.component.*">
	<fx:Declarations>
		<!-- 将非可视元素（例如服务、值对象）放在此处 -->
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			private function init():void
			{
				
			}
		]]>
	</fx:Script>
	<mx:Panel width="100%" height="100%"  styleName="customPanel" title="传阅列表"  borderVisible="false" dropShadowVisible="false">
		<mx:VBox width="100%" height="100%" id="listPanel">
			<mx:VBox width="100%" height="25%"  verticalGap="10" verticalAlign="middle" horizontalAlign="center" >
				<mx:VBox width="100%" height="100%"   horizontalGap="10" verticalAlign="middle" horizontalAlign="left" borderVisible="true" paddingTop="0" paddingBottom="0"   paddingLeft="10" paddingRight="10">
					<receipt:searchBox id="search_panel"/>
				</mx:VBox>
			</mx:VBox>
			<mx:VBox width="100%" height="75%"  verticalAlign="middle" horizontalAlign="center" paddingTop="20">
				<receipt:datagrid id="list"/>
			</mx:VBox>
		</mx:VBox>
	</mx:Panel>
</mx:Module>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="system.entity.MessageModel" table="wlzx_system_message">
		<id name="id">
			<column name="id" /> 
			<generator class="native" />
		</id>
		<property name="fromId" type="string">
			<column name="from_id" />
		</property>
		<property name="toIds" type="string">
			<column name="to_ids" />
		</property>
		<property name="readFlags" type="string">
			<column name="read_flags" />
		</property>
		<property name="type" type="int">
			<column name="type" />
		</property>
		<property name="content" type="string">
			<column name="content" />
		</property>
		<property name="creationDate" type="java.util.Date">
			<column name="creation_date" />
		</property>
		<property name="readDates" type="string">
			<column name="read_dates" />
		</property>
		<property name="postRemovedFlag" type="string">
			<column name="postRemoved_flag" />
		</property>
		<property name="receiveRemovedFlags" type="string">
			<column name="receiveRemoved_flags" />
		</property>
	</class>
</hibernate-mapping>

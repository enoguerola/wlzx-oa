<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="system.entity.DataAccessModeModel" table="wlzx_system_data_access_mode">
		<id name="id">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="symbol" type="string" unique="true">
			<column name="symbol" />
		</property>
		<property name="sequence" type="int">
			<column name="sequence" />
		</property>
		<property name="name" type="string">
			<column name="name" />
		</property>
		<property name="detail" type="string">
			<column name="detail" />
		</property>
		<property name="value" type="string">
			<column name="value" />
		</property>
		<property name="creationDate" type="java.util.Date">
			<column name="creation_date" />
		</property>
		<property name="modifiedDate" type="java.util.Date">
			<column name="modified_date" />
		</property>
		<set name="operations" lazy="false"  table="wlzx_system_operation_dam_relationship" cascade="all" inverse="true">
			<key column="dam_id" not-null="true" />
			<many-to-many column="operation_id" class="system.entity.OperationModel" /> 
		</set>
	</class>
</hibernate-mapping>
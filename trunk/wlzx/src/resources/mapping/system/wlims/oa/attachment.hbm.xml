<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="system.wlims.oa.entity.AttachmentModel" table="wlzx_oa_attachment">
        <id name="id">
            <column name="id" />
            <generator class="native" />
        </id>

        <property name="name" type="string">
            <column name="name"/>
        </property>
        
        <property name="type" type="string">
            <column name="type" />
        </property>
        
        <property name="path" type="string">
            <column name="path" />
        </property>
        <property name="size" type="string">
            <column name="size" />
        </property>
        <property name="date" type="java.util.Date">
	            <column name="upload_date" />
	     </property>
		
        
        <property name="belongObject" type="string">
            <column name="belong_object" />
        </property>
         <property name="belongObjectId" type="string">
            <column name="belong_object_id" />
        </property>
        
    </class>
</hibernate-mapping>
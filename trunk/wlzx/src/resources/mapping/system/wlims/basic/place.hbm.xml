<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="system.wlims.basic.entity.PlaceModel" table="wlzx_basic_place">
		<id name="id">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="name" type="string">
			<column name="name" />
		</property>
		<property name="position" type="string">
			<column name="position" />
		</property>
		<property name="type" type="string">
			<column name="type" />
		</property>
		
		<property name="symbol" type="string">
			<column name="symbol" />
		</property>
		<property name="functions" type="string">
			<column name="functions" />
		</property>
		<property name="areaBegin" type="float">
			<column name="area_begin" />
		</property>
		<property name="areaEnd" type="float">
			<column name="area_end" />
		</property>
		<property name="containBegin" type="int">
			<column name="contain_begin" />
		</property>
		<property name="containEnd" type="int">
			<column name="contain_end" />
		</property>
		
		<property name="remarks" type="string">
			<column name="remarks" />
		</property>
		<property name="creationDate" type="java.util.Date">
			<column name="creation_date" />
		</property>
		<property name="modifiedDate" type="java.util.Date">
			<column name="modified_date" />
		</property>
		
	</class>
</hibernate-mapping>
<?xml version="1.0" encoding="utf-8"?>
<xls-fields data-row-index="4" column-row-index="2" column-row-span="2">
	<field-title rowspan="2">
		<field-cell>A1</field-cell>
		<field-text>详细信息</field-text>
		<field-key-data>xls.title</field-key-data>
	</field-title>
	<field-columns>
		<field-column>
			<field-cell>A</field-cell>
			<field-text>姓名</field-text>
			<field-key-data>base.userName</field-key-data>
		</field-column>
		<field-column>
			<field-cell>B</field-cell>
			<field-text>主部门</field-text>
			<field-key-data>base.mainDepartmentName</field-key-data>
		</field-column>
		<field-column>
			<field-cell>C</field-cell>
			<field-text>职务</field-text>
			<field-key-data>base.mainRoleName</field-key-data>
		</field-column>
		<field-column>
			<field-cell>D</field-cell>
			<field-text>电子邮箱</field-text>
			<field-key-data>base.email</field-key-data>
		</field-column>
		<field-column>
			<field-cell>E</field-cell>
			<field-text>办公电话</field-text>
			<field-key-data>base.officePhone</field-key-data>
		</field-column>
		<field-column>
			<field-cell>F</field-cell>
			<field-text>手机号码</field-text>
			<field-key-data>base.mobilePhone</field-key-data>
		</field-column>
		
		
	</field-columns>
</xls-fields>

<?xml version="1.0" encoding="utf-8"?>
<xls-fields data-row-index="4" column-row-index="2" column-row-span="2">
	<field-title rowspan="2">
		<field-cell>A1</field-cell>
		<field-text>详细信息</field-text>
		<field-key-data>xls.title</field-key-data>
	</field-title>
	<field-columns>
		<field-column>
			<field-cell>A</field-cell>
			<field-text>组别</field-text>
			<field-key-data>user.group</field-key-data>
		</field-column>
		<field-column>
			<field-cell>B</field-cell>
			<field-text>姓名</field-text>
			<field-key-data>base.name</field-key-data>
		</field-column>
		<field-column>
			<field-cell>C</field-cell>
			<field-text>短号</field-text>
			<field-key-data>base.teacherNo</field-key-data>
		</field-column>
		<field-column>
			<field-cell>D</field-cell>
			<field-text>手机号</field-text>
			<field-key-data>base.teacherMobilePhone</field-key-data>
		</field-column>
		<field-column>
			<field-cell>E</field-cell>
			<field-text>办电</field-text>
			<field-key-data>base.teacherOfficePhone</field-key-data>
		</field-column>
		<field-column>
			<field-cell>F</field-cell>
			<field-text>电子邮箱</field-text>
			<field-key-data>base.teacherMail</field-key-data>
		</field-column>
		<field-column>
			<field-cell>G</field-cell>
			<field-text>家庭地址</field-text>
			<field-key-data>base.address</field-key-data>
		</field-column>
		<field-column>
			<field-cell>H</field-cell>
			<field-text>家庭电话</field-text>
			<field-key-data>base.homePhone</field-key-data>
		</field-column>
		<field-column>
			<field-cell>I</field-cell>
			<field-text>紧急联系电话</field-text>
			<field-key-data>base.teacherContactPhone</field-key-data>
		</field-column>
		<field-column>
			<field-cell>J</field-cell>
			<field-text>性别</field-text>
			<field-key-data>base.sexText</field-key-data>

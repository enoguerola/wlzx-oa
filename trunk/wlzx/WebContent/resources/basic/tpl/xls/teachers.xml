		<field-column>
			<field-cell>X</field-cell>
			<field-text>入本单位时间</field-text>
			<field-key-data>base.teacherAttendDate</field-key-data>
		</field-column>
		<field-column>
			<field-cell>Y</field-cell>
			<field-text>技能等级</field-text>
			<field-key-data>base.teacherSkillLevel</field-key-data>
		</field-column>
		<field-column>
			<field-cell>Z</field-cell>
			<field-text>技能鉴定时间</field-text>
			<field-key-data>base.enterpriseSeniorTechnicianCount</field-key-data>
		</field-column>
		<field-column>
			<field-cell>AA</field-cell>
			<field-text>其他部门</field-text>
			<field-key-data>base.enterpriseSeniorTechnicianCount</field-key-data>
			<field-key-comment>otherDepartments.data</field-key-comment>
		</field-column>
		<field-column>
			<field-cell>AB</field-cell>
			<field-text>工作经验</field-text>
			<field-key-data>base.enterpriseSeniorTechnicianCount</field-key-data>
			<field-key-comment>relations.data</field-key-comment>
		</field-column>
		<field-column>
			<field-cell>AC</field-cell>
			<field-text>社会关系</field-text>
			<field-key-data>base.enterpriseSeniorTechnicianCount</field-key-data>
			<field-key-comment>experiences.data</field-key-comment>
		</field-column>
